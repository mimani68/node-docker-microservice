{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Docker: Attach to Node",
            "port": 9229,
            "sourceMaps": true,
            "protocol": "inspector",
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "/usr/src",
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js",
                "<node_internals>/**/*.js",
            ]
        }
    ]
}